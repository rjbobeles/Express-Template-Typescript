{
  "name": "express-template-typescript",
  "version": "0.0.1",
  "description": "express-template-typescript",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=prod node ./build/index.js",
    "dev": "cross-env NODE_ENV=dev ts-node ./src/index.ts --files",
    "dev:nodemon": "cross-env NODE_ENV=dev nodemon -w src -e ts,json -x ts-node ./src/index.ts",
    "test": "cross-env NODE_ENV=mocha mocha --exit",
    "test:watch": "cross-env NODE_ENV=mocha mocha --watch",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:down": "knex migrate:down",
    "migrate:currentVersion": "knex migrate:currentVersion",
    "migrate:unlock": "knex migrate:unlock",
    "migrate:status": "knex migrate:list",
    "seed:make": "knex seed:make",
    "seed:run": "knex seed:run"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/hpp": "^0.2.1",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.45",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "sinon": "^11.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "husky": "^6.0.0",
    "knex": "^0.21.17",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "objection": "^2.2.15"
  }
}
